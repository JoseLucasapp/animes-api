{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "allowJs": true, /* Allow 'import x from y' when a module doesn't have a default export. */
    "esModuleInterop": true, /* Disable resolving symlinks to their realpath. This correlates to the same flag in node. */
    "forceConsistentCasingInFileNames": true, /* Skip type checking .d.ts files that are included with TypeScript. */
    "skipLibCheck": true,
  },
  "include": [
    "src/**/*.ts"
  ]
}